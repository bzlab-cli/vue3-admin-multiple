{
  "name": "vue3-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel mock serve:dev",
    "serve:dev": "cross-env NODE_ENV=development dotenv -e .env.develop vue-cli-service serve",
    "build:deploy": "cross-env NODE_ENV=production dotenv -e .env.deploy vue-cli-service build",
    "build:release": "cross-env NODE_ENV=production dotenv -e .env.release vue-cli-service build",
    "build": "cross-env NODE_ENV=production dotenv -e .env.prod vue-cli-service build",
    "preview": "node build/index.js --preview",
    "lint": "vue-cli-service lint",
    "mock": "cd mock && ts-node-dev mock.ts"
  },
  "dependencies": {
    "@bz/bz-upload": "0.1.6",
    "@bzlab/bz-core": "^0.1.2",
    "axios": "^0.21.1",
    "dayjs": "^1.10.7",
    "echarts": "^5.2.2",
    "element-plus": "^1.0.2-beta.35",
    "file-saver": "^2.0.5",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "screenfull": "^5.0.2",
    "vue": "^3.2.6",
    "vue-router": "4.0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/js-cookie": "^2.2.6",
    "@types/koa": "^2.11.6",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/koa2-cors": "^2.0.1",
    "@types/lodash": "^4.14.167",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^14.14.11",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-eslint": "~4.5.12",
    "@vue/cli-plugin-router": "~4.5.12",
    "@vue/cli-plugin-typescript": "~4.5.12",
    "@vue/cli-plugin-vuex": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-polyfill": "^1.0.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.2",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "koa2-cors": "^2.0.6",
    "lint-staged": "^9.5.0",
    "log4js": "^6.3.0",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "runjs": "^4.4.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
