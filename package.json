    "style-loader": "^2.0.0",
    "style-resources-loader": "^1.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-style-resources-loader": "^0.1.4"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
